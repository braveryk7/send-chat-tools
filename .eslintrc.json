{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended",
		"plugin:import/warnings"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"sourceType": "module"
	},
	"plugins": [
		"import"
	],
	"rules": {
		"import/order": [
			"warn",
			{
				"alphabetize": {
					"order": "asc"
				},
				"newlines-between": "always"
			}
		]
	}
}